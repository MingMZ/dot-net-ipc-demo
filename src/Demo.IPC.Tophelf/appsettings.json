{
  "Serilog": {
    "Using": [ 
      "Serilog.Sinks.Literate", "Serilog.Sinks.File" ],
    "MinimumLevel": "Verbose",
    "WriteTo": [{
        "Name": "LiterateConsole"
      },{
        "Name": "File",
        "Args": {
          "path": ".\\log.txt",
          "fileSizeLimitBytes": 1234567
        }
      }
    ]
  }
}
